							<field name="search_elements" 
								size="30"
								type="listfields"
								label="COM_FABRIK_SEARCH_ELEMENTS_LABEL"/>
						
						</fieldset>
				</fields>
				
			</field>
		</fieldset>
	
		<fieldset name="layout">
			
